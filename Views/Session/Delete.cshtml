@model Session
<h1>Session</h1>
<form method="post" asp-action="Delete">

    <div class="bBooking p-3 mt-4">
        <div class="row pb-2">
            <h1 class="text-primary">Delete Session</h1>
        </div>
        <div class="mb-2">
            <label>Name</label>
            <input asp-for="SessionName" disabled class="form-control" />
        </div>
        <div class="mb3">
            <label >Description</label>
            <input asp-for="SessionDescription" disabled class="form-control"/>
        </div>
        <br />
        <button type="submit" class="btn btn-primary">Delete</button>
        <a asp-controller="Session" asp-action="Index" class="btn btn-primary">Back To List</a>
    </div>
</form>

